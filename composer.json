{
    "name": "joelbutcher/laravel-googleads",
    "description": "Laravel Service Provider for the Google Ads Client OAuth2 Wrapper",
    "keywords": ["googleads", "laravel", "google", "advertising"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/joelbutcher/laravel-googleads",
        "source": "https://github.com/joelbutcher/laravel-googleads"
    },
    "authors": [
        {
            "name": "Joel Butcher",
            "email": "joel@joelbutcher.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "joelbutcher/googleads": "dev-patch-to-support-ads-api-13"
    },
    
      "repositories": [

        {
            "type": "vcs",
            "url": "https://github.com/prageeth-peiris/googleads"
        }


    ],
    
    "autoload": {
        "psr-4": {
            "JoelButcher\\LaravelGoogleAds\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoelButcher\\LaravelGoogleAds\\ServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
